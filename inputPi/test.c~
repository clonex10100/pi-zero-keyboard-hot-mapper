#include <stdlib.h>
#include <unistd.h>
#include <fcntl.h>
#include <errno.h>
#include <linux/input.h>
#include <string.h>
#include <stdio.h>

void outputPrep(char out[23], int mods, int keys[6]);

int main(void){
	int keys[6];
	int mods = 45;
	char out[23];
	keys[0] = 10;
	keys[1] = 4;
	keys[2] = 430;
	keys[3] = 430;
	keys[4] = 100;
	keys[5] = 4;
	keys[6] = 300;
	outputPrep(out,mods,keys);
	printf("%s\n",out);
}
void outputPrep(char out[23], int mods, int keys[6]){
	char temp[4];
	int len;
	for(int i = 0; i < 6; i++){
		printf("cycle :%i\n",i);
		if(i == 0){
			len = sprintf(temp,"%i",mods);
		}
		else{
			len = sprintf(temp,"%i",keys[i-1]);
		}
		if(len == 1){
			out[i*4] = '0';
			out[i*4+1] = '0';
			out[i*4+2] = temp[0];
		}
		else if(len == 2){
			out[i*4] = '0';
			out[i*4+1] = temp[0];
			out[i*4+2] = temp[1];
		}
		else{
			out[i*4] = temp[0];
			out[i*4+1] = temp[1];
			out[i*4+2] = temp[2];
		}
		if(i + 1 < 6){
			out[i*4+3] = ':';
			out[i*4+4] = '\0';
		}
		else{
			out[i*4+3] = '\0';
		}
		printf("%s\n",out);
	}
	printf("%s\n",out);
}
